//swapping numbers
#include<stdio.h>
int main()
{
    double first, second, temp;
    printf("Enter first number:");
    scanf("%lf", &first);
    printf("Enter second number:");
    scanf("%lf", &second);
    temp=first;
    first=second;
    second=temp;
    //second=temp=first;
    printf("\nAfter swapping first number=%lf", first);
    printf("\nAfter swapping, the second number= %lf", second);
    
    return 0;
}
