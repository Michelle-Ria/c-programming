#include<stdio.h>

int main()
{
    //Variable declarations
    int intVar=10;
    short shortVar=5;
    //short variable
    float floatVar=5.5F;
    //float variable
    double doubleVar=20.99;
    //double variable
    char charVar= 'A';
    //charecter variable(charecter variables alwasy have to be in single quotes)
    long longVar=
    //long variable
    
}
