//Fahrenheit to celcius conversion
#include<stdio.h>
#include<conio.h>

int main()

 {
     float Fahrenheit, celcius;
     printf("Enter temperature value( in Fahrenheit):");
     scanf("%f", &Fahrenheit);
     celcius=(Fahrenheit-32)/1.8;
     printf("\nEquivalent Temperature (in celcius)= %0.2f", celcius);
     getch();
     return 0;
 }
